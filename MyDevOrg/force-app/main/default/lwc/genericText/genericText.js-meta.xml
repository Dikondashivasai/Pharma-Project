            <property name="isRequiredField" type="Boolean" label="isRequiredField" role="inputOnly" description="Contains whether this field is required or not" />
            <property name="valueEntered" label="valueEntered" type="String" description="The text value provided and returned from the control" />
            <property name="valueEnteredNum" label="valueEnteredNum" type="Integer" description="The Number value provided and returned from the control" />
