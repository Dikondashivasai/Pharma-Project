            <property name="step" label="Decimal Step" type="String" role="inputOnly" />
            <property name="toolTipText" label="toolTipText" type="String" role="inputOnly" />
            <property name="unitRate" label="unit Rate" type="String" />
            <property name="qty" label="qty" type="String"  />
            <property name="manufacturing" label="manufacturing" type="String" />
            <property name="laboratory" label="laboratory" type="String"/>
            <property name="totalcy" label="totalcy" type="String"/>
            <property name="serviceType" label="serviceType" type="String"/>
            <property name="prospectType" label="prospectType" type="String" />
            <property name="displaytrue" label="displaytrue" type="Boolean" default="true" />
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>